{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "rootDir": "./",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "outDir": "./dist",
    "sourceMap": true
   },
  "exclude": [
    "dist",
    "node_modules"
  ]
}